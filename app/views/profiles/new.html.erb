  <%= stylesheet_link_tag    'form', media: 'all' %>

<div class="wrapper fadeInDown">
<div id="formContent">

<%= form_with url: "/profiles/new", method: "post" do |f| %>

<%= f.text_field :name, placeholder: "Full Name"  %>

<%= f.text_field :age, placeholder: "Age" %>

<%= f.text_field :university, placeholder: "University" %>

<%= f.text_field :city, placeholder: "City" %>

<%= f.text_field :bio, placeholder: "Short Introduction"  %>

<%= f.text_field :major, placeholder: "major"  %>
<%= f.label "Upload Profile Picture:" %><br>
<%= f.file_field :image  %>
<br>
<br>

<%= f.label :programming_languages %><br>
<%= f.collection_check_boxes :language_ids, Language.all, :id, :name %>
<br>
<br>

<%= f.collection_select :cohort_id, Cohort.all, :id, :name %>

<br>
<%= f.submit "Create Profile"%>

<% end %>
</div>
</div>